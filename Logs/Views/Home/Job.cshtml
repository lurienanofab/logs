@using Logs.Models;

@model IEnumerable<JobModel>
@{
    ViewBag.Title = "Job";
}

<div class="page-header">
    <h1>
        @Html.ActionLink("Logs", "Index") / Job
    </h1>
</div>

@if (Model.Count() == 0)
{
    <div class="alert alert-warning" role="alert">
        No items were found.
    </div>
}
else
{
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Path</th>
                <th>Result</th>
                <th>Timestamp</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Path</td>
                    <td>@item.Result</td>
                    <td>@item.Timestamp</td>
                </tr>
            }
        </tbody>
    </table>
}